{
  "name": "cloud-kruser-backend",
  "packageManager": "yarn@3.2.3",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "package:all": "yarn workspaces foreach run package",
    "build:all": "yarn workspaces foreach run build"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.179.0",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/node": "^18.7.23",
    "@yarnpkg/sdks": "^3.0.0-rc.18",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "husky": "^8.0.1",
    "nx": "^14.6.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "[[ -n $HUSKY_BYPASS ]] || commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependenciesMeta": {
    "typescript@4.8.3": {
      "unplugged": true
    }
  }
}
